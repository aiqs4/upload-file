{
  "name": "upload-file",
  "version": "1.0.0",
  "description": "Upload files to github",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sd-hd/upload-file.git"
  },
  "keywords": [
    "upload",
    "file",
    "github",
    "action",
    "node"
  ],
  "author": "Sascha Dewald",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sd-hd/upload-file/issues"
  },
  "homepage": "https://github.com/sd-hd/upload-file#readme",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^3.4.1",
    "eslint-plugin-jest": "^23.8.0",
    "jest": "^25.1.0",
    "jest-circus": "^25.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@actions/core": "^1.9.1",
    "actions-toolkit": "^3.0.1"
  }
}
